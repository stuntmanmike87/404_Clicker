{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "allowJs": true,
        "strict": false,
        "resolveJsonModule": true,
        "lib": [ "es2015", "es2016", "dom" ],
        "noImplicitAny": true,
        "sourceMap": true,
        "outDir": "./dist",
        "skipLibCheck": true
    },
    "exclude": [
        "node_modules"
    ],
    "include": ["**/*.ts"],
}